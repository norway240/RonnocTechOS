local logo = paintutils.loadImage("rtos/rtlogo")
local os = paintutils.loadImage("rtos/rtos")
paintutils.drawImage(os,1,1)
sleep(1)

for i = 1,25 do
  loadimg = paintutils.loadImage("rtos/rtosl/rtosl"..i)
  paintutils.drawImage(loadimg, 1,1)
  sleep(0.25)
end

shell.run("monpic","rtos/rtlogo","top")
paintutils.drawImage(logo,1,1)
sleep(5)
shell.run("rtshell")
