print("Please Login")
write("User: ")
local user = read()
if fs.isDir("/rtos/users/"..user) == false then
  print("That user does not exist!")
else
  local userpassget = fs.open("/rtos/users/"..user.."/pass", "r")
  local userpass = userpassget.readAll()
  write("Pass: ")
  local pass = read()
  if pass ~= userpass then
    print("Wrong Password!")
  else
    print("Logged In!")
  end
end